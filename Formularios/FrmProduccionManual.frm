Private Sub cmdAgregar_Click()
On Error GoTo TipoErrs
 Dim MontoProduccion As Double, CodEmpleado As Double, Fecha As String
 
  If Me.DBCodigoEmpleado.Text = "" Then
   MsgBox "Se necesita el Codigo Empleado", vbCritical, "Sistema de Nominas"
   Exit Sub
  End If

  If Not IsNumeric(Me.TxtMonto.Text) Then
   MsgBox "Es necesario digiar Numerico para el Monto", vbCritical, "Sistema de Nominas"
   Exit Sub
  End If
  
   Fecha = Format(Me.DTFecha.Value, "yyyy-mm-dd")
  CodEmpleado = Me.DBCodigoEmpleado.Columns(0).Text
  MontoProduccion = Me.TxtMonto.Text
  
  Me.AdoBusca.RecordSource = "SELECT  * From DetalleProduccionManual Where (CodEmpleado = " & CodEmpleado & ") And (FechaProduccion = CONVERT(DATETIME, '" & Fecha & "', 102))And (Pagado = 0)"
  Me.AdoBusca.Refresh
  If Not Me.AdoBusca.Recordset.EOF Then
    MsgBox "Este Registro Existe", vbCritical, "Sistema de Nominas"
    Exit Sub
  End If
  
  
  Me.AdoAgrega.Recordset.AddNew
   Me.AdoAgrega.Recordset("CodEmpleado") = CodEmpleado
   Me.AdoAgrega.Recordset("FechaProduccion") = Format(Me.DTFecha.Value, "dd/mm/yyyy")
   Me.AdoAgrega.Recordset("MontoProduccion") = Me.TxtMonto.Text
  Me.AdoAgrega.Recordset.Update
  
Me.AdoDetalleProduccion.Refresh

Exit Sub
TipoErrs:
 MsgBox Err.Description

End Sub

Private Sub cmdBorrar_Click()
 Dim CodEmpleado As Double, Fecha As String
 
 CodEmpleado = Me.TDBProduccion.Columns(0).Text
 
 Fecha = Format(Me.TDBProduccion.Columns(1).Text, "yyyy-mm-dd")
 
 
 Me.AdoBusca.RecordSource = "SELECT  * From DetalleProduccionManual Where (CodEmpleado = " & CodEmpleado & ") And (FechaProduccion = CONVERT(DATETIME, '" & Fecha & "', 102))And (Pagado = 0)"
 Me.AdoBusca.Refresh
  If Not Me.AdoBusca.Recordset.EOF Then
  Me.AdoBusca.Recordset.Delete
   
 End If
 
 Me.AdoDetalleProduccion.Refresh
End Sub

Private Sub DBCodigoEmpleado_ItemChange()
 Dim CodEmpleado As Double, Fecha As String
 CodEmpleado = Me.DBCodigoEmpleado.Columns(0).Text


 Me.AdoDetalleProduccion.RecordSource = "SELECT  * From DetalleProduccionManual Where (CodEmpleado = " & CodEmpleado & ") And (Pagado = 0)"
 Me.AdoDetalleProduccion.Refresh
 
 Me.AdoDetalleProduccion.Refresh
 
End Sub

Private Sub Form_Load()
MDIPrimero.Skin1.ApplySkin hWnd

 With Me.AdoDetalleProduccion
  .ConnectionString = Conexion
  .RecordSource = "SELECT   * From DetalleProduccionManual Where (NumNomina = -1)"
  .Refresh
 End With
 
  With Me.AdoBusca
  .ConnectionString = Conexion
 End With
 
  With Me.AdoAgrega
  .ConnectionString = Conexion
  .RecordSource = "DetalleProduccionManual"
  .Refresh
 End With
 
 With Me.AdoEmpleados
   .ConnectionString = Conexion
   .RecordSource = "SELECT CodEmpleado, CodEmpleado1, Activo, Nombre1 + ' '+ Nombre2 +' '+Apellido1+' '+Apellido2 as Nombres From Empleado Where (Activo = 1) ORDER BY CodEmpleado1"
   .Refresh
End With

Me.DTFecha.Value = Now

Me.DBCodigoEmpleado.Columns(0).Visible = False
Me.DBCodigoEmpleado.Columns(1).Caption = "Codigo"
Me.DBCodigoEmpleado.Columns(1).Width = 800
Me.DBCodigoEmpleado.Columns(2).Visible = False
 
End Sub
